<SOUI trCtx="dlg_set_skin" title="set skin" width="214" height="220" margin="5,5,5,5" resizable="0" translucent="1" toolwindow="1">
  <style>
    <!--局部style对象-->
    <class name="cls_border" ncSkin="_skin.sys.border" margin-x="2" margin-y="2" />
  </style>
  <skin>
    <imglist name="skin_bkimg_0_fit" src="bkimg:skin_pic_0" colorizeEnable="0"/>
    <imglist name="skin_bkimg_1_fit" src="bkimg:skin_pic_1" colorizeEnable="0"/>
    <imglist name="skin_bkimg_2_fit" src="bkimg:skin_pic_2" colorizeEnable="0"/>
    <imglist name="skin_bkimg_3_fit" src="bkimg:skin_pic_3" colorizeEnable="0"/>
    <imglist name="skin_bkimg_4_fit" src="bkimg:skin_pic_4" colorizeEnable="0"/>
    <imglist name="skin_bkimg_5_fit" src="bkimg:skin_pic_5" colorizeEnable="0"/>
    <imglist name="skin_bkimg_6_fit" src="bkimg:skin_pic_6" colorizeEnable="0"/>
    <imglist name="skin_bkimg_7_fit" src="bkimg:skin_pic_7" colorizeEnable="0"/>
  </skin>
  <root class="cls_dlg_frame" cache="1">
    <window pos="5,5">
      <img pos="0,0,@100,@50" class="cls_border" name="img_skin_0" id="10" msgTransparent="0" skin="skin_bkimg_0_fit"/>
      <img pos="[2,0,@100,@50" class="cls_border" name="img_skin_1" id="11" msgTransparent="0" skin="skin_bkimg_1_fit"/>
    </window>
    <window pos="5,[2">
      <img pos="0,0,@100,@50" class="cls_border" name="img_skin_2" id="12" msgTransparent="0" skin="skin_bkimg_2_fit"/>
      <img pos="[2,0,@100,@50" class="cls_border" name="img_skin_3" id="13" msgTransparent="0" skin="skin_bkimg_3_fit"/>
    </window>
    <window pos="5,[2">
      <img pos="0,0,@100,@50" class="cls_border" name="img_skin_4" id="14" msgTransparent="0" skin="skin_bkimg_4_fit"/>
      <img pos="[2,0,@100,@50" class="cls_border" name="img_skin_5" id="15" msgTransparent="0" skin="skin_bkimg_5_fit"/>
    </window>
    <window pos="5,[2">
      <img pos="0,0,@100,@50" class="cls_border" name="img_skin_6" id="16" msgTransparent="0" skin="skin_bkimg_6_fit"/>
      <img pos="[2,0,@100,@50" class="cls_border" name="img_skin_7" id="17" msgTransparent="0" skin="skin_bkimg_7_fit"/>
    </window>
  </root>
</SOUI>
